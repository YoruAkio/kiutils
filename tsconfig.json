{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "sourceMap": true,
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "declaration": true,
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
