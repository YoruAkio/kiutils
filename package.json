        "canvas",
        "thumbnails",
        "waifu",
        "anime",
        "sourcebin",
        "code-sharing",
        "typescript",
        "javascript",
        "nodejs"
    ],
    "dependencies": {
        "@napi-rs/canvas": "^0.1.41",
        "axios": "^1.4.0",
        "canvacord": "^5.4.10",
